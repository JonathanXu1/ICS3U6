import java.util.Scanner;
import java.io.File;
import java.io.PrintWriter;

public class Summative {
  public static void main(String[] args) throws Exception{
    Scanner input1 = new Scanner(System.in);
    String response;
    
    // Welcome
    System.out.println("Welcome to the monster hunter mini world!");
    System.out.print("Enter your filename (including .txt extension");
    response = input1.nextLine();
    
    //Initialize
    File file = new File(name);
    Scanner input2 = new Scanner(file);
    int[][] world = loadMap(input2);
    
    //Results
    for (int i = 0; i < world.length; i++){
      for(int j = 0; j < world.length; j++){
        System.out.print(world[i][j]);
      }
      System.out.println();
    }
    
    input1.close();   
    input2.close();
  }
  
  public static int[][] loadMap(String name) throws Exception{
    String line;
    String symbol;
    line = input2.nextln();
    
    int[][] map = new int[line.length()][line.length()];
    map[0][0] = line;
    
    for (int i = 1; i < count; i++){
      line = input2.nextLine();
      for (int j = 1; j < count; j++){
        symbol = line.substring(j, j+1);
        if ("+-|".contains(symbol)){
          map[i][j] = 1;
        }
        else if (symbol.equals("N")){
          map[i][j] = 2;
        }
        else if (symbol.equals("F")){
          map[i][j] = 3;
        }
        else if (symbol.equals("P")){
          map[i][j] = 4;
        }
      }
    }
    
    return map;
  }
}