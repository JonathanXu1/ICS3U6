class Output7{
  public static void main(String[]args){
    System.out.println("a)");
    System.out.prinln("Hello");
    System.out.prinln("Hello");
    System.out.prinln("Hello");
    System.out.prinln("Hello");
    System.out.prinln("Hello");
    System.out.prinln("b)");
    for(int i=0, i<=5, i++){
      System.out.println("Hello");
    }
  }
}