abstract class Organism{
  private int health;
  private int age = 0;
  
  Organism(int init){
    this.health = init;
  }
  
  void changeHealth(int h){
    this.health += h;
  }
  
  int getHealth(){
    return this.health;
  }
  
  int getAge(){
    return age;
  }
  
  void setAge(int a){
    this.age = a;
  }
}